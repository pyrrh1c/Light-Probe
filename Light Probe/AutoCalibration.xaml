<Window x:Class="Light_Probe.AutoCalibration"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Light_Probe"
        mc:Ignorable="d"
        Title="AutoCalibration" Height="125" Width="300" Icon="Arduino-Icon-32.ico">
    <Grid>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Label Content="Automatic Calibration Coming Soon..."/>
            <Button Name="BT_CloseAutoCalibration" Content="Close" HorizontalAlignment="Center" Width="75" Click="BT_CloseAutoCalibration_Click"/>
        </StackPanel>
    </Grid>
</Window>
